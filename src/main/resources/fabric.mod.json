{
  "schemaVersion": 1,
  "id": "examplemod",
  "version": "${version}",

  "name": "${modName}",
  "description": "${modDescription}",
  "authors": [
    "${modAuthor}"
  ],
  "contributors": [
    "${modCredits}"
  ],
  "contact": {
    "homepage": "${modHomepage}",
    "sources": "${modSource}",
    "issues": "${modIssues}"
  },
  "license": "CC0 1.0 Universal",
  "environment": "*",
  "entrypoints": {
    "main": [
      "net.darkhax.examplemod.ExampleMod"
    ]
  },
  "mixins": [
    "examplemod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  }
}